<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="question-set-heading">Question sets</h1>
            {{#each this.questionset}}
            <ul class="list-group">
                <li class="list-group-item d-flex justify-content-between align-items-center">
                    <img src="/img/{{questionset_image}}" width="80git px" height="80px" class="img-rounded">
                    <h4>{{questionset_title}}</h4>
                        <span class="badge">
                            <label id ="status-question-set">Active</label>
                            <button class="btn btn-success"> <i class="fas fa-external-link-alt"></i> New Game </button>
                            <button class="btn btn-info mx-4"><i class="fas fa-edit"></i> Edit</button>
                       </span>
                </li>
            </ul>
            {{/each}}
        </div>
    </div>
    <a class="btn btn-primary btn-lg fas fa-plus-circle" id ="btn-question-set" style="margin-top: 20px" href="/questionset/add"> Add</a>
</div>